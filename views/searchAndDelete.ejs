<%- include("partials/header"); -%>

  <div class="container" style="margin-top: 10px">
  <h1 style="margin-bottom: 15px">Delete a Question</h1>
  <h4>Current Questions IDs</h4>
  <% if(currentQuestions) { %>
  <% currentQuestions.forEach(function(currentQuestion) { %>
  <ul>
    <li><%=currentQuestion%></li>
  </ul>
  <% }); %>
  <% } %>
  <% if(!(currentQuestions)) { %>
  <h4>No current Questions</h4>
  <% } %>
    <form  action="/searchAndDelete" method="post">
      <div class="form-group">
        <label>Enter the Unique Question ID</label>
        <input class="form-control" type="text" name="uniqueID" autocomplete="off" required>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Search</button>
    </form>
  </div>
  </body>
 
 <%- include("partials/footer"); -%>